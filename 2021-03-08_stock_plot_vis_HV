    "# Davide's Code\n",
    "\n",
    "df_list = [[nasdaq100, 'nasdaq100'], \n",
    "           [pharma, 'pharma'], \n",
    "           [industry, 'industry'], \n",
    "           [tourism, 'tourism'], \n",
    "           [dax, 'dax'], \n",
    "           [hero, 'hero'], \n",
